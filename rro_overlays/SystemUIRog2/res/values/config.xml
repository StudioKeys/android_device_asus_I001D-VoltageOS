<resources>
    <!-- Color of the UDFPS pressed view -->
    <color name="config_udfpsColor">#00000000</color>

    <!-- HBM type of UDFPS overlay.
            0 - GLOBAL HBM 
            1 - LOCAL HBM
    -->
    <integer name="config_udfpsHbmType">0</integer>

    <!-- The radius of the enrollment progress bar, in dp -->
    <integer name="config_udfpsEnrollProgressBar" translatable="false">135</integer>

    <!-- Doze: does this device support STATE_DOZE?  -->
    <bool name="doze_display_state_supported">true</bool>

    <!-- Doze: does this device support STATE_DOZE_SUSPEND?  -->
    <bool name="doze_suspend_display_state_supported">true</bool>

</resources>
